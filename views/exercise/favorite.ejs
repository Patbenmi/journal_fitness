<h1>Favorite Exercises</h1>

<ol></ol>
<% exercises.forEach(exercise =>{ %>
  <div class="exerciseList">
    <li>
      <a href="/exercise/<%= exercise.apiId %> "><%= exercise.exerciseName %></a>
      <form action="/exercise/favorite/<%= exercise.apiId %>/?_method=DELETE" method="POST">
        <input hidden type="integer" name="id" value="<% exercise.apiId %> ">
        <input type="submit" value="Delete Exercise">
      </form>
    </li>
    </div>
  <% }) %> 
</ol>